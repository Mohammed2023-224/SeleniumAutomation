<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Practice Suite" parallel="tests" thread-count="2">
    <listeners>
        <listener class-name="engine.listeners.TestNgListener"/>
        <listener class-name="engine.listeners.TransformListener"/>
    </listeners>
    <test name="edge tests">
        <parameter name="browser" value="edge"/>
        <classes>
            <class name="tests.AutomationPlaygroundTests"/>
        </classes>
    </test> <!-- Test -->
    <test name="chrome tests ">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="tests.AutomationPlaygroundTests"/>
        </classes>
    </test> <!-- Test -->
</suite> <!-- Suite -->